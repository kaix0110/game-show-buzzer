            let iStockPixelSearchData = JSON.parse('{\u0022sponsor_pixel\u0022:\u0022freepik\u0022,\u0022sponsor\u0022:\u0022freepik\u0022,\u0022term\u0022:\u0022\u0022,\u0022r\u0022:0.5772067553257602}');
            iStockPixelSearchData.action = 'clk';
