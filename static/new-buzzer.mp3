        <template x-data x-if="Alpine.store('scriptLoader')?.googleLogin">
            <script>
setTimeout(() => {
    showGoogleOneTap();
}, 5000);

function showGoogleOneTap() {
    google.accounts.id.initialize({
        client_id: '705648808057-3chuddbr6oahbebib1uh693k02sgfl30.apps.googleusercontent.com',
        callback: handleCredentialsResponse,
        cancel_on_tap_outside: false,
        prompt_parent_id: '#credential_picker_container',
        itp_support: true,
        context: 'use'
    });

    google.accounts.id.prompt(
        (notification) => {
            if (notification.isNotDisplayed()) return;
        }
    );
}

function handleCredentialsResponse(credentials) {
    const postData = {
        credential: credentials.credential
    };

    const hint = JWTDecode(credentials.credential);

    if (hint.email_verified === true) {
        localStorage.setItem(
            'id.videvo.net.auth.hints',
            JSON.stringify({
                email: hint.email,
                email_verified: hint.email_verified,
                picture: hint.picture
            })
        );
    }

    const requestOptions = {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify(postData)
    };

    fetch(
        `https://id.freepik.com/api/v2/login/google-one-tap?client_id=videvo&lang=${getCookie('gr_lang')}&no-redirect=1`,
        requestOptions
        )
        .then(response => {
            if (!response.ok) {
                throw new Error('Network response was not ok..');
            }
            
            return response.json();
        })
        .then(data => {
            if (!data.success || !data.data.redirectUrl) {
                throw new Error('Redirect URL not found in response data..');
            }

            window.location.href = data.data.redirectUrl;
        })
        .catch(error => {
            console.error(error);
        });
}

function JWTDecode(token) {
    let base64Url = token.split('.')[1];
    let base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');
    const jsonPayload = decodeURIComponent(window.atob(base64).split('').map(function(c) {
        return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
    }).join(''));

    return JSON.parse(jsonPayload);
}

function getCookie(name) {
    const value = `; ${document.cookie}`;
    const parts = value.split(`; ${name}=`);

    if (parts.length === 2) return parts.pop().split(';').shift();
}

function getStorageItem(key) {
    return JSON.parse(localStorage.getItem(key) || null);
}
</script>        </template>
    
